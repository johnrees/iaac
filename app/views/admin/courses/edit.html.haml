.row
  .columns.large-12
    %ul.breadcrumbs
      %li= link_to "Courses", admin_courses_path
      - @course.ancestors.each do |course|
        %li= link_to course, [:admin,course]
      %li= link_to @course, [:admin,@course]

.row
  .columns.large-12
    = title "Edit Course"
    = render "form", course: @course
